class Solution {
    public int[][] transpose(int[][] matrix) {

        int matrix2[][] = new int[matrix[0].length][matrix.length];

        for(int i =0 ; i< matrix2.length; i++){
            for(int j = 0; j<matrix2[0].length; j++){
                matrix2[i][j] = matrix[j][i];
            }
        }        

        return matrix2;
    }
}